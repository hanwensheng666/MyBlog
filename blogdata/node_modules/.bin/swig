#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_swig@1.4.2@swig/bin/swig.js" "$@"
  ret=$?
else 
  node  "$basedir/../_swig@1.4.2@swig/bin/swig.js" "$@"
  ret=$?
fi
exit $ret
